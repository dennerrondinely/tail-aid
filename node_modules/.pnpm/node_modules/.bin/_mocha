#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/denners/Personal/tailaid/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/bin/node_modules:/Users/denners/Personal/tailaid/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/node_modules:/Users/denners/Personal/tailaid/node_modules/.pnpm/mocha@5.2.0/node_modules:/Users/denners/Personal/tailaid/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/denners/Personal/tailaid/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/bin/node_modules:/Users/denners/Personal/tailaid/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/node_modules:/Users/denners/Personal/tailaid/node_modules/.pnpm/mocha@5.2.0/node_modules:/Users/denners/Personal/tailaid/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mocha/bin/_mocha" "$@"
else
  exec node  "$basedir/../mocha/bin/_mocha" "$@"
fi
